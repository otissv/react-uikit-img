{
  "name": "react-uikit-img",
  "version": "0.1.1",
  "description": "React uikit image component for responsive or cover images.",
  "keywords": [
    "javascript",
    "react",
    "uikit",
    "react-component",
    "react-uikit",
    "uikit-react",
    "img"
  ],
  "repository": "git@github.com:otissv/react-uikit-img.git",
  "author": "Otis Virginie",
  "license": "MIT",
  "main": "./lib/img.js",
  "scripts": {
    "docs": "babel-node ./reactToHtml.js ./docs/img-doc > README.md && date",
    "docs:watch": "watch 'npm run docs' ./docs ",
    "build": "babel -d lib/ src/ && date",
    "build:watch": "watch 'npm run build' ./src",
    "lint": "eslint --quiet ./src/**   && date|| true",
    "lint:watch": "watch 'npm run lint' ./",
    "prepublish": "babel -d lib/ src/ && date",
    "test": "babel-node test-runner.js | tap-min  && date|| true",
    "test:spec": "babel-node test-runner.js | tap-difflet  && date|| true",
    "test:watch": "watch 'npm run test' ./"
  },
  "devDependencies": {
    "babel-core": "~6.2.1",
    "babel-eslint": "~4.1.5",
    "babel-plugin-transform-runtime": "~6.1.18",
    "babel-preset-es2015": "~6.1.18",
    "babel-preset-react": "~6.1.18",
    "babel-preset-stage-0": "~6.1.18",
    "babel-runtime": "~6.2.0",
    "blue-tape": "~0.1.10",
    "cheerio": "~0.19.0",
    "cuid": "~1.3.8",
    "eslint": "~1.10.1",
    "eslint-config-standard": "~4.4.0",
    "eslint-plugin-react": "~3.11.2",
    "eslint-plugin-smells": "~1.0.1",
    "eslint-plugin-standard": "~1.3.1",
    "faucet": "~0.0.1",
    "glob": "~6.0.1",
    "history": "~1.13.1",
    "react-addons-test-utils": "~0.14.3",
    "react-dom": "~0.14.3",
    "tap-difflet": "~0.4.0",
    "tap-min": "~1.0.0",
    "watch": "~0.16.0"
  },
  "dependencies": {
    "react": "~0.14.3",
    "react-uikit-base": "~0.1.1",
    "ufunc": "~0.2.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "engines": {
    "node": "~4.2.3"
  }
}
